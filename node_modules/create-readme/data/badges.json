    "url": "https://bundlephobia.com/result?p={{name}}"
  },
  "npm-downloads": {
    "name": "downloads",
    "img": "https://img.shields.io/npm/dw/{{name}}.svg",
    "url": "https://npmjs.org/package/{{name}}"
  },
  "open-issues": {
    "name": "open issues",
    "img": "https://img.shields.io/github/issues-raw/{{git.user}}/{{git.repo}}.svg",
