    "url": "https://github.com/semantic-release/semantic-release#badge"
  },
  "greenkeeper": {
    "name": "Greenkeeper",
    "img": "https://badges.greenkeeper.io/{{git.user}}/{{git.repo}}.svg",
    "url": "https://greenkeeper.io/"
  },
  "debug": {
    "name": "debug",
    "img": "https://img.shields.io/badge/debug-blue.svg",
